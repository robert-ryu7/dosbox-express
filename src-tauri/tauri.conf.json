{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "dosbox-express",
    "version": "../package.json"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "open": true,
        "message": true,
        "confirm": true
      },
      "shell": {
        "all": true
      },
      "path": {
        "all": true
      },
      "process": {
        "exit": true
      }
    },
    "bundle": {
      "active": false,
      "category": "Entertainment",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "org.solid-dev.dosbox-express",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "DOSBox Express",
        "minWidth": 640,
        "minHeight": 480,
        "width": 800,
        "height": 600
      }
    ]
  }
}
